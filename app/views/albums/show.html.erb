<h1><%= @album.title %></h1>
<h2>By: <%= @album.band.name %></h2>
<p>Year:<%= @album.year %></p>


<a href="<%= band_url(@album.band.id) %>">Back to band</a>
<a href="<%= edit_album_url(@album.id) %>">Edit this album</a>
<form action="<%= album_url(@album.id) %>" method="POST">
    <%= auth_token %>
    <input type="hidden" name="_method" value="DELETE">
    <input type="submit" value="Delete this album">
</form>